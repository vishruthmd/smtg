{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next",
            "config": {
                "includeFiles": [
                    "next.config.js",
                    "next.config.mjs",
                    "next.config.ts"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/inngest",
            "dest": "/api/inngest"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
